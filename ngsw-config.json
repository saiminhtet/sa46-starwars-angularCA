{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls":[
          "https://fonts.googleapis.com/**"
        ]

      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups":[
    {
      "name": "starwars-api",
      "urls": ["https://swapi.co/api/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 30,
        "maxAge": "1h",
        "timeout": "5s"
      }
    }
  ]
}
